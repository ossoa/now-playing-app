{
  "rules": {
    "no-console": [0]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  parserOptions: {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
    },
    sourceType: "module"
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
